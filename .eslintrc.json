{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier", "prettier/prettier"],
  "parser": "@typescript-eslint/parser",
  "ignorePatterns": "dist",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-console": "off"
  }
}
